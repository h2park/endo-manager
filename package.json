{
  "name": "endo-manager",
  "version": "2.0.10",
  "description": "endo-manager",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "coverage": "nyc npm test",
    "build": "env NODE_ENV=production webpack --config webpack.config.prod.js -p",
    "start": "webpack-dev-server --config webpack.config.dev.js --port 3000 --history-api-fallback",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "mocha:json": "mocha --reporter=json > coverage/mocha.json",
    "test": "NODE_ENV=test mocha",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "keywords": [
    "zooid",
    "zooid-app",
    "react",
    "babel",
    "webpack"
  ],
  "homepage": "https://github.com/sqrtofsaturn/endo-manager",
  "bugs": "https://github.com/sqrtofsaturn/endo-manager/issues",
  "author": {
    "name": "Octoblu",
    "url": "https://github.com/sqrtofsaturn"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sqrtofsaturn/endo-manager.git"
  },
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "css-loader": "^0.27.3",
    "eslint-plugin-babel": "^3.1.0",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.9.0",
    "filesize": "^3.3.0",
    "gzip-size": "^3.0.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "lodash": "^4.11.1",
    "postcss-loader": "^0.11.0",
    "precss": "^1.4.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-icons": "^2.1.0",
    "react-router": "^2.0.1",
    "recursive-readdir": "^2.0.0",
    "rimraf": "^2.5.2",
    "superagent": "^2.2.0",
    "url": "^0.11.0",
    "webpack": "^2.2.1",
    "zooid-error-state": "^1.2.0",
    "zooid-ui": "^8.0.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-subset": "^1.3.0",
    "coffee-script": "^1.11.1",
    "cross-env": "^2.0.0",
    "css-modules-require-hook": "^4.0.6",
    "eslint": "^3.17.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.13.4",
    "jsdom": "^9.12.0",
    "mocha": "^2.5.3",
    "mocha-multi": "^0.9.1",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^10.1.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "webpack-dev-server": "^2.4.1"
